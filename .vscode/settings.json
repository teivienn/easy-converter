{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": false,
  "editor.trimAutoWhitespace": true,
  "files.insertFinalNewline": true,
  "files.encoding": "utf8",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "cSpell.words": [
    "CBRF",
    "NBRB",
    "Valute",
    "Valutes"
  ]
}
